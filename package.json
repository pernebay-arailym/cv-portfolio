{
  "name": "cv-portfolio",
  "homepage": "http://pernebay-arailym.github.io/cv-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/cv-portfolio.umd.cjs",
  "module": "./dist/cv-portfolio.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cv-portfolio.js",
      "require": "./dist/cv-portfolio.umd.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "netlify": "netlify",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-require": "^0.0.3",
    "vite-svg-loader": "^5.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "netlify": "^13.1.11",
    "netlify-cli": "^17.8.1",
    "owl.carousel": "^2.3.4",
    "react-owl-carousel": "^2.3.3",
    "react-toastify": "^9.1.3",
    "rxjs": "^7.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
